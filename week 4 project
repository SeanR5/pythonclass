problem (1)
def message():
  count=0
  while (count < 5):     
    count = count+1
    print("A message") 
message()

problem(3)
def square(num):
    return num * num
number = float(input("Number to square"))
sqr = square(number)
print("The answer is {0}".format(sqr))

